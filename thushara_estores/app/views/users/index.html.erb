<table border="1">
	
	<tr>
		<th>Name</th>
        <th>Email</th>
        </tr>
        
        <% @user.each do| user |  %>
        
        
        <tr>
        	 <td><%=user.name %></td>
        	 <td><%=user.email %></td>
        	 <td><%=link_to"edit",edit_user_path(user)%></td>
        	 <td><%=link_to"show",edit_user_path(user)%></td>
        	 <td><%=link_to"Delete",user, method: :delete, data:{confirm:"Are you sure:"}%></td>
        </tr>
        
        	
        	
<%end%>

<tr>
	<td><%=link_to "Add User",new_user_path%></td>
</tr>


</table>
